            <Setter Property="Width" Value="18"/>
            <Setter Property="Height" Value="18"/>
            <Setter Property="Margin" Value="4,2,0,0"/>
            <Setter Property="Stroke" Value="{StaticResource Excircle}"/>
        </Style>
