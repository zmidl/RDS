        <LinearGradientBrush x:Key="BackgroundFill" EndPoint="0.5,1" StartPoint="0,0">
            <GradientStop Color="#FF4B4B4B" Offset="1"/>
            <GradientStop Color="White"/>
            <GradientStop Color="#FF939393" Offset="0.364"/>
            <GradientStop Color="#FFB8B4B4" Offset="0.703"/>
        </LinearGradientBrush>
        
        <LinearGradientBrush x:Key="Content" EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#33444444" Offset="1"/>
            <GradientStop Color="#194D4D4D"/>
        </LinearGradientBrush>

        <LinearGradientBrush x:Key="Excircle" EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="White" Offset="0.712"/>
        </LinearGradientBrush>

        <!--<SolidColorBrush x:Key="Excircle" Color="Transparent"/>-->

