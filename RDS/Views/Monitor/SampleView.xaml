        <common:SamplingToSolidColorBrush x:Key="SamplingToSolidColorBrush"/>
        <common:SampleSelectionStateToSolidColorBrush x:Key="SampleSelectionStateToSolidColorBrush"/>
        
        <DataTemplate x:Key="Column0Header">
            <StackPanel Orientation="Horizontal">
               <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.TempM, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column1Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding Mode=TwoWay,  Path=DataContext.IsNeedToExportTempIPM, RelativeSource={RelativeSource AncestorType=Window}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.TempIPM, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column2Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding Mode=TwoWay, Path=DataContext.IsNeedToExportFaultCode, RelativeSource={RelativeSource AncestorType=Window}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.FaultCode, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column3Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding Mode=TwoWay, Path=DataContext.IsNeedToExportDrumSpeed, RelativeSource={RelativeSource AncestorType=Window}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.DrumSpeed, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column4Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding Mode=TwoWay, Path=DataContext.IsNeedToExportLoad, RelativeSource={RelativeSource AncestorType=Window}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.Load, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column5Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding Mode=TwoWay, Path=DataContext.IsNeedToExportOOB, RelativeSource={RelativeSource AncestorType=Window}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding Path=DataContext.OOB, RelativeSource={RelativeSource AncestorType=Window}}" />
            </StackPanel>
        </DataTemplate>
        <Style x:Key="SingleTubeStyle" TargetType="UserControl">
            <Setter Property="Width" Value="50"/>
            <Setter Property="Height" Value="50"/>
            <Setter Property="Margin" Value="10,0,0,0"/>
        </Style>

        <Style x:Key="DataGridColumn0Editing" TargetType="TextBox">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="Background" Value="Gray"/>
            <Setter Property="BorderBrush" Value="Navy"/>
            <Setter Property="BorderThickness" Value="2"/>
        </Style>

