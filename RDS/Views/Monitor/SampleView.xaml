        <common:SamplingToSolidColorBrush x:Key="SamplingToSolidColorBrush"/>
        <common:SampleSelectionStateToSolidColorBrush x:Key="SampleSelectionStateToSolidColorBrush"/>

        <DataTemplate x:Key="Column0Header">
            <StackPanel Orientation="Horizontal">
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.TempM, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column1Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding DataContext.IsNeedToExportTempIPM, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.TempIPM, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column2Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding DataContext.IsNeedToExportFaultCode, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.FaultCode, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column3Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding DataContext.IsNeedToExportDrumSpeed, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.DrumSpeed, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column4Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding DataContext.IsNeedToExportLoad, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.Load, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="Column5Header">
            <StackPanel Orientation="Horizontal">
                <zmlc2:WG_CheckBox Width="15" Height="15" IsChecked="{Binding DataContext.IsNeedToExportOOB, Mode=TwoWay, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
                <Label Width="2"/>
                <TextBlock Width="Auto"  Text="{Binding DataContext.OOB, RelativeSource={RelativeSource AncestorType={x:Type Window}}}" />
            </StackPanel>
        </DataTemplate>
        <Style x:Key="SingleTubeStyle" TargetType="{x:Type UserControl}">
            <Setter Property="Width" Value="50"/>
            <Setter Property="Height" Value="50"/>
            <Setter Property="Margin" Value="10,0,0,0"/>
        </Style>

        <Style x:Key="DataGridColumn0Editing" TargetType="{x:Type TextBox}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="Background" Value="Gray"/>
            <Setter Property="BorderBrush" Value="Navy"/>
            <Setter Property="BorderThickness" Value="2"/>
        </Style>

